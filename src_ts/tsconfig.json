{
  "compilerOptions": {
    "outDir": "./build",
    "lib": ["es2015.iterable", "es2015.generator", "es5", "dom"],
    "target": "es5",
    "rootDir": ".",
    "esModuleInterop": true,

    "downlevelIteration": true,

    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictPropertyInitialization": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,

    "declarationMap": false,
    "sourceMap": false,
    "composite": false,
    "declaration": true,
    "stripInternal": true
  }
}
